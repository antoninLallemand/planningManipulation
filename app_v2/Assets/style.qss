/*-----QWidget-----*/
QWidget
{
	background-color: #606060;
	color: #ffffff;
	border-color: #051a39;
	font-weight: bold;
	font-family : Trebuchet MS;
	font-size : 20px;

}

#MainVLayout{
	border: 1px solid red;
	width: 100%;
	height: 100%;
}

QPushButton {
	background-color: rgb(57, 57, 196);
	border-radius: 5px;
	padding: 10px;
}

QLineEdit, QComboBox {
	background-color: #8f8a8a;
	border-radius: 5px;
	border: 2px solid rgb(57, 57, 196);
}